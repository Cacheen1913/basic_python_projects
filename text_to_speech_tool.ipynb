   "source": [
    "import pyttsx3\n",
    "import time\n",
    "names = [\"sachin\", \"aditya\", \"souvik\"]\n",
    "\n",
    "for i in names:\n",
    "    speaker = pyttsx3.init()\n",
    "    speaker.say(f\"Hello {i}, how are you today?\")\n",
    "    speaker.runAndWait()\n",
    "    time.sleep(0.2)\n",
    "    del speaker"
   ]
